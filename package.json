    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.3.0",
    "joi-objectid": "^3.0.1",
    "joi-password-complexity": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.14",
    "mongoose-type-url": "^2.1.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.18",
    "randomstring": "^1.1.5",
    "react-infinite-scroll-component": "^6.0.0",
    "react-scripts": "^4.0.3",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.5"
